<!-- views/partials/cards.ejs -->
<div class="col-md-4 col-sm-6 col-12 col-lg-3">
  <div class="card">
    <img src="<%= cards.itemImage %>" class="card-img-top pic" alt="" />
    <div class="card-body">
      <h5 class="m-0"><%= cards.itemName %></h5>
      <p class="m-0"><%= cards.itemPrice %></p>
      <button class="btn bg-warning mt-1 pl-3 pr-3 py-1">
        View
      </button>
    </div>
  </div>
</div>
